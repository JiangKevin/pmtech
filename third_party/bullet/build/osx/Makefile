# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  bullet_monolithic_config = debug
endif
ifeq ($(config),release)
  bullet_monolithic_config = release
endif

PROJECTS := bullet_monolithic

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

bullet_monolithic:
ifneq (,$(bullet_monolithic_config))
	@echo "==== Building bullet_monolithic ($(bullet_monolithic_config)) ===="
	@${MAKE} --no-print-directory -C . -f bullet_monolithic.make config=$(bullet_monolithic_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f bullet_monolithic.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   bullet_monolithic"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"