struct vs_output
{
	float4 position : SV_POSITION0;
	float2 tex_coord: TEXCOORD0;
};

struct ps_output
{
	float4 colour : SV_Target;
};

Texture2D tex;
SamplerState samp;

ps_output main( vs_output input )
{
	ps_output output;
	
	output.colour = tex.Sample( samp, input.tex_coord.xy );
	
    return output;
}