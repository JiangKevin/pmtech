struct vs_input
{
	float4 position : POSITION;
	float2 tex_coord: TEXCOORD0;
};

struct vs_output
{
	float4 position : SV_POSITION;
	float2 tex_coord: TEXCOORD0;
};

vs_output main( vs_input _input )
{
	vs_output _output;
	
	_output.position = _input.position;
	_output.tex_coord = _input.tex_coord;
	
    return _output;
}